---
name: deploy-stockflow
on:
 push:
   branches: [ main ]
jobs:
 build:
   name: deploy-stockflow
   runs-on: self-hosted
   steps:
   - name: Checkout
     uses: actions/checkout@v2
 
   - name: deploy-microservice
     #env:
     #  PASS: ${{ secrets.MYSQL_PASS }}
     run: |
       bash deploy_project.sh
